  println("Add1: "+Add1+Add2);
}

4.g
String address = "Mørdrupvej";
int addressNum = 45+45+3;
int cityCode = 9180/3;
String besked = "Her bor jeg nu!";

//Nye Tilføjelser
char husBogstav = 'b';
String by = " Espergærde";

//4.f,g,h
float Add1 = cityCode+1;
float Add2 = addressNum+1;

float add3 = cityCode+3;
float add4 = addressNum+3;

float substract1 = cityCode-1;
float substract2 = addressNum-1;
void setup(){
  println("Add3: "+add3+add4);
}

