  println("Adresse: "+Address);
  println("Adresse Nummer: "+AddressNum);
  println("By Kode: "+CityCode);
  println("Kommentar: "+besked);
}

4.e
String Address = "Mørdrupvej";
int AddressNum = 45+45+3;
int CityCode = 9180/3;
String besked = "Her bor jeg nu!";

//Nye Tilføjelser
char HusBogstav = 'b';
String By = " Espergærde";

void setup(){
  println("Adresse: "+Address);
  println("Adresse Nummer: "+AddressNum+HusBogstav);
  println("By Kode: "+CityCode +By);
  println("Kommentar: "+besked);
}

