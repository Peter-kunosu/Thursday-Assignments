----- STUDENT -----
class Student {
  String name;
  int age;
  boolean isFemale;
  int datamatikerTeam;


  Student(String tmpName, int tmpAge, boolean tmpIsFemale, int tmpDatamatikerTeam) {
    name = tmpName;
    age = tmpAge;
    isFemale = tmpIsFemale;
    datamatikerTeam = tmpDatamatikerTeam;
  }
}

----- LÆRER -----
class Teacher {
  String name;
  int age;
  boolean isFemale;
  int datamatikerTeam;

  
  Teacher(String tmpName, int tmpAge, boolean tmpIsFemale, int tmpDatamatikerTeam) {
    name = tmpName;
    age = tmpAge;
    isFemale = tmpIsFemale;
    datamatikerTeam = tmpDatamatikerTeam;
  }
}

----- DATAMATIK/ void setup -----
void setup() {
  // 3.i 
  Teacher myTeacher = new Teacher("Anders", 40, false, 1);
  Teacher classmateTeacher = new Teacher ("Bente", 35, true, 2);
  
  // 3.j
  Student me = new Student("Peter", 21, false, 1);
  Student classmate = new Student("Julie", 22, true, 2);

  // 3.k
  println("Teacher: " + myTeacher.name + ", Team " + myTeacher.datamatikerTeam);
  println("Teacher: " + classmateTeacher.name+ ", Team " + classmateTeacher.datamatikerTeam);
  
  // 3.l
  println("Student 1: " + me.name + ", Team " + me.datamatikerTeam);
  println("Student 2: " + classmate.name + ", Team " + classmate.datamatikerTeam);
}

----- Hvad der printes -----
Teacher: Anders, Team 1 // Team 1's lærer
Teacher: Bente, Team 2 // Team 2's lærer
Student 1: Peter, Team 1 // Jeg er på team 1
Student 2: Julie, Team 2 // Julie er på team 2
