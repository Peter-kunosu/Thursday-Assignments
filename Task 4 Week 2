4.a // Kode som tæller fra 0 til 20
void setup(){
  for (int tæl = 0; tæl<=20; tæl++){
  println(tæl);
  }
}

4.b // kode som kun tæller lige tal fra 0 til 20
void setup(){
  for (int i = 0; i<=20; i += 2){
  println(i);
  }
}

4.c og 4.d
for(int start = 3; start >= 0; start--){
  switch(start){
    case 3:
    println("Three!");
    break;
    case 2:
    println("Two!");
    break;
    case 1:
    println("One!");
    break;
    case 0:
    println("TAKE OFF!");
    break;
  }
}

